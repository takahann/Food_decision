<%= form_for @user do |f| %>
  <div class="field">
    <%= f.label :name %>
    <%= f.text_field :name %>
  </div>
  <div class="field">
    <%= f.label :email %>
    <%= f.text_field :email %>
  </div>
  <div class="field">
    <%= f.label :profile_image %>
    <%= f.attachment_field :profile_image %>
  </div>
    <%= f.submit %>
<% end %>
